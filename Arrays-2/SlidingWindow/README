for(end : tillArrayLast)
{

	//Look for end condition 

	if(end meets some condition)
	{
		//Increment start for ex: start++;
	}
}


Dynamic Window

for(end : tillArrayLast)
{

	//Look for end condition 

	while(end meets some condition)
	{
		//Increment start for ex: start++;
	}
}
